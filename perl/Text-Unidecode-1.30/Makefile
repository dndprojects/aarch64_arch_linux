	$(NOECHO) $(ECHO) '    <ABSTRACT>Provide plain ASCII transliterations of Unicode text</ABSTRACT>' >> Text-Unidecode.ppd
	$(NOECHO) $(ECHO) '    <AUTHOR>Sean Burke &lt;sburke@cpan.org&gt;</AUTHOR>' >> Text-Unidecode.ppd
	$(NOECHO) $(ECHO) '    <IMPLEMENTATION>' >> Text-Unidecode.ppd
	$(NOECHO) $(ECHO) '        <PERLCORE VERSION="5,008,0,0" />' >> Text-Unidecode.ppd
